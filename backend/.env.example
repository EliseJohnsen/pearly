# Local development (SQLite)
DATABASE_URL=sqlite:///./perle.db

# Railway PostgreSQL (Production)
# DATABASE_URL=${{Postgres.DATABASE_URL}}
# Railway automatically injects this variable when you link the PostgreSQL service
BACKEND_CORS_ORIGINS=["http://localhost:3000","https://your-app.vercel.app"]
UPLOAD_DIR=./uploads
REPLICATE_API_TOKEN=your_replicate_api_token_here
SANITY_PROJECT_ID=your_sanity_project_id
SANITY_DATASET=production
SANITY_API_TOKEN=your_sanity_api_token_with_write_access
SANITY_API_VERSION=2024-12-16
SANITY_WEBHOOK_SECRET=your_sanity_web_hook_here
SECRET_KEY=secret_key_here
